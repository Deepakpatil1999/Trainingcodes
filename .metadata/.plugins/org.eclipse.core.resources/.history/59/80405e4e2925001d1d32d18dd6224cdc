package com.attra.assignments;

public class Calculator {
	int a=4, b=2 ;
	public void addition()
	{
		int add=a+b;
		System.out.println("Addition of number : " + add );
	}
	public void subtraction()
	{
		int sub=a+b;
		System.out.println("Addition of number : " + sub );
	}

	public void multplication ()
	{
		int mul=a+b;
		System.out.println("Addition of number : " + mul );
	}

	public void division()
	{
		int div=a+b;
		System.out.println("Addition of number : " + div );
	}
public static void main(String[] args) {
	Calculator cal=new Calculator();
	cal.addition();
	cal.subtraction();
	cal.multplication();
	cal.division();
	
}
	

}
