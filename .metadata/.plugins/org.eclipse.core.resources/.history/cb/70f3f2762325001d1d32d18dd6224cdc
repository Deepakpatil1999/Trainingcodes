package com.attra.assignments;

public class LargestNumber {
	public static void main(String[] args) {
		int a=5, b=10, c=20;
		if(a>b)
		{
			System.out.println("Then a is largest number");
		}
		else
		{
			System.out.println("Then a is not larger then b");
		}
		if(b>c)
		{
			System.out.println("Then b is largest number");
		}
		else
		{
			System.out.println("Then b is not larger then c");
		}
		if(c>a)
		{
			System.out.println("Then c is largest number" +c);
		}
		else
		{
			System.out.println("Then c is not larger then a" );
		}
		
	}

}
