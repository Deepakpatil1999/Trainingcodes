package com.attra.training;

import java.util.Scanner;

public class Demoforifelse {
	
	public static void main(String[] args) {
		int x=0;
		Scanner sc = new Scanner(System.in);
		
		System.out.println("Hi");
		
	
	if(x==sc.nextInt())
	{
		System.out.println("type");
	}
	else
	{
		System.out.println("Hi");
	}

}

}
